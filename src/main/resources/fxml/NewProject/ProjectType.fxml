<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="545.0" prefWidth="658.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.controller.NewProject.ProjectType">
   <top>
      <HBox alignment="CENTER_RIGHT" prefHeight="75.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <TextField promptText="search project" style="-fx-background-color: transparent; -fx-prompt-text-fill: #485460;">
               <HBox.margin>
                  <Insets bottom="20.0" left="5.0" right="10.0" top="20.0" />
               </HBox.margin>
            </TextField>
            <FontAwesomeIconView glyphName="SEARCH" selectionFill="#485460" size="1.5em" styleClass="cursor_hand">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin></FontAwesomeIconView>
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER_RIGHT" minHeight="-Infinity" prefHeight="100.0" prefWidth="612.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="PreviousButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#goBack" styleClass="buttonStyle" stylesheets="@../../css/button.css" text="Previous">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="NextButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#goForword" styleClass="buttonStyle" stylesheets="@../../css/button.css" text="Next">
               <HBox.margin>
                  <Insets bottom="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
